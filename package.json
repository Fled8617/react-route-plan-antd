{
  "name": "react-route-plan-antd",
  "version": "0.0.1",
  "homepage": ".",
  "repository": {
    "type": "git",
    "url": "https://github.com/Fled8617/react-route-plan-antd"
  },
  "dependencies": {
    "antd": "^3.7.1",
    "axios": "^0.18.0",
    "mobx": "^4.3.1",
    "mobx-react": "^5.0.0",
    "moment": "^2.22.2",
    "react-google-maps": "^9.4.5",
    "recompose": "^0.30.0",
    "scriptjs": "^2.5.9"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ]
  },
  "babel": {
    "babelrc": false,
    "presets": [
      "react-app"
    ],
    "plugins": [
      "styled-components",
      [
        "import",
        {
          "libraryName": "antd",
          "libraryDirectory": "es",
          "style": "css"
        }
      ],
      [
        "transform-decorators-legacy"
      ],
      [
        "transform-do-expressions"
      ]
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "devDependencies": {
    "@antv/g6": "^2.1.0",
    "antd-mobile": "^2.1.4",
    "autoprefixer": "7.1.6",
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.2",
    "babel-plugin-import": "^1.8.0",
    "babel-plugin-styled-components": "^1.5.1",
    "babel-preset-react-app": "^3.1.1",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "css-loader": "0.28.7",
    "dotenv": "4.0.0",
    "dotenv-expand": "4.2.0",
    "draft-js": "^0.10.5",
    "draftjs-to-html": "^0.8.4",
    "eslint": "4.10.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.4.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "fs-extra": "3.0.1",
    "gh-pages": "^2.0.1",
    "html-to-draftjs": "^1.4.0",
    "html-webpack-plugin": "2.29.0",
    "immutability-helper": "^2.8.1",
    "jest": "20.0.4",
    "jquery": "^3.3.1",
    "jspdf": "^1.4.1",
    "less": "^3.8.0",
    "less-loader": "^4.1.0",
    "mockjs": "^1.0.1-beta3",
    "node-sass": "^4.9.2",
    "node-xlsx": "^0.12.1",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "promise": "8.0.1",
    "raf": "3.4.0",
    "rc-color-picker": "^1.2.6",
    "rc-queue-anim": "^1.6.4",
    "rc-tween-one": "^2.0.11",
    "react": "^16.4.1",
    "react-beautiful-dnd": "^7.1.3",
    "react-color": "^2.14.1",
    "react-contextmenu": "^2.9.3",
    "react-dev-utils": "^5.0.1",
    "react-dnd": "^5.0.0",
    "react-dnd-html5-backend": "^5.0.1",
    "react-dom": "^16.4.1",
    "react-draft-wysiwyg": "^1.12.13",
    "react-form-builder-antd": "^0.0.1",
    "react-free-scrollbar": "^0.3.2",
    "react-intl": "^2.4.0",
    "react-lz-editor": "^0.12.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "reactcss": "^1.2.3",
    "resolve": "1.6.0",
    "sass-loader": "^7.0.3",
    "sortablejs": "^1.7.0",
    "style-loader": "0.19.0",
    "styled-components": "^3.4.2",
    "sw-precache-webpack-plugin": "0.11.4",
    "tui-color-picker": "^2.2.0",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-plugin-transform-do-expressions": "^6.22.0",
    "babel-polyfill": "^6.26.0",
    "mobx-react-devtools": "^6.0.3"
  }
}
